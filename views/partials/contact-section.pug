-var contact = widgets.contact.data
section.contact-home.line-sec.green
    .container-one
        .row
            .contact-data
                h2 Kontakt
                .firm-name.margin-bottom-32 
                    b !{contact.firmName}

                .margin-bottom-32

                a(href="mailto:"+contact.email) !{contact.email}

                .margin-bottom-32

                .address.margin-bottom-32 !{contact.address}

                each ph,i in contact.firstPhones
                    .phone !{ph}

                .margin-bottom-32

                each ph,i in contact.secondPhones
                    .phone !{ph}

            .contact-account
                h3 Nasze konta:
                each b,i in contact.banks
                    p !{b.name}
                    each ac in b.accounts
                        p !{ac}

            .contact-logo
                .logo-padding
                    img(src="/img/logo.svg" class="img-fluid")

